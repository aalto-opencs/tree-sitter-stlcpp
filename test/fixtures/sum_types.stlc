// Test sum types at type level
unwrap : (Unit + Int) -> Int
unwrap = fun opt : Unit + Int, case opt of
    | inl x => 0
    | inr n => n

// Test nested sum types
either : (Int + Bool) -> Int
either = fun x : Int + Bool, case x of
    | inl n => n
    | inr b => if b then 1 else 0

// Test sum types in function signatures
parse_result : [Char] + (Unit, [Char])
parse_result = inl [: Char] (Unit, [Char])
